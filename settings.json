{
    "workbench.colorTheme": "Gruvbox Dark Hard",
    "workbench.iconTheme": "gruvbox-material-icon-theme",
    "workbench.startupEditor": "none",
    "cSpell.language": "en,pt,pt_BR",
    "cSpell.enableFiletypes": [
        "log",
        "shellscript",
        "sql"
    ],
    "editor.formatOnPaste": true,
    "diffEditor.wordWrap": "on",
    "editor.wordWrap": "on",
    "terminal.external.linuxExec": "xfce4-terminal",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.defaultProfile.linux": "zsh",
    "editor.fontFamily": " 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "terminal.integrated.fontSize": 10,
    //"terminal.integrated.fontFamily": "MesloLGS NF, Regular",
    "cSpell.userWords": [
        "autofetch",
        "cpptools",
        "esbenp",
        "gruvbox",
        "Gruvbox",
        "javac",
        "jnbt",
        "Meslo",
        "redhat",
        "Setti",
        "xfce"
    ],
    "explorer.confirmDelete": false,
    "security.workspace.trust.untrustedFiles": "open",
    "python.defaultInterpreterPath": "python",
    "[python]": {
        "editor.formatOnType": true,
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "python.formatting.provider": "none",
    "[javascript]": {
        "editor.formatOnType": true,
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.formatOnType": true,
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "ruby.interpreter.commandPath": "ruby",
    "rufo.exe": "~/.rvm/gems/ruby-3.2.1/bin/rufo",
    "rufo.useBundler": false,
    "[ruby]": {
        "editor.formatOnType": true,
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "jnbt.vscode-rufo"
    },
    "[html]": {
        "editor.formatOnType": true,
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[cpp]": {
        "editor.formatOnType": true,
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "ms-vscode.cpptools",
        "editor.wordBasedSuggestions": false,
        "editor.suggest.insertMode": "replace",
        "editor.semanticHighlighting.enabled": true
    },
    "[java]": {
        "editor.formatOnType": true,
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "redhat.java"
    },
    "git.autofetch": true,
    "explorer.compactFolders": false,
    "code-runner.ignoreSelection": true,
    "code-runner.runInTerminal": true,
    "code-runner.clearPreviousOutput": true,
    "code-runner.executorMap": {
        "python": "clear && python -u",
        "javascript": "node",
        "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
        "ruby": "ruby",
        "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "shellscript": "bash",
        "sass": "sass --style expanded",
        "scss": "scss --style expanded"
    },
    "window.zoomLevel": 2
}